{
    "name": "FZ Token Changer",
    "short_name": "FZ/VZ/SZ Token Changer",
    "version": "0.1.1",
    "version_name": "0.1.1",
    "offline_enabled": false,
    "homepage_url": "https://github.com/StarterX4/factorio-zone-change-token-ext",
    "author": "StarterX4, Rubydesic",
    "description": "Change token on Factorio.zone, Valheim.zone and Satisfactory.zone",
    "manifest_version": 3,
    "icons": {
		"16": "icons/icon_16.png",
		"24": "icons/icon_24.png",
		"32": "icons/icon_32.png",
		"48": "icons/icon_48.png",
		"64": "icons/icon_64.png",
		"128": "icons/icon_128.png",
		"256": "icons/icon_256.png",
		"512": "icons/icon_512.png"
	},
    "content_scripts": [{
        "matches": [ "https://factorio.zone/*", "https://valheim.zone/*", "https://satisfactory.zone/*" ],
        "js": [ "js/sweetalert2.all.min.v10.js", "js/factoriozone-token.js" ]
                        }]
}
